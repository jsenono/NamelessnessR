<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.NamelessnessR" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <item name="android:statusBarColor" tools:targetApi="l">?android:windowBackground</item>
        <item name="android:navigationBarColor">?android:windowBackground</item>
    </style>

    <style name="Theme.NamelessnessR.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.NamelessnessR.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.NamelessnessR.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>